
.serach {
  margin-top: 18rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
  background-color: #F3F3F3;
  border-radius: 12rpx;
}

.serach_view_show {
  display: block;
  color: #808080;
  font-size: 18rpx;
  background-color: #F3F3F3;
  border-radius: 12rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx; 
  text-align: center;
}

.item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 8rpx;
  margin-left: 68rpx;
  margin-right: 68rpx;
  margin-top: 13rpx;
  margin-bottom: 13rpx;
  background-color: #FFFFFF;
}

.item_left {
  margin-top: 10rpx;
  display: block;
  align-content: center;
}

.item_middle {
  margin-top: 10rpx;
  display: block;
  align-content: center;
}

.item_right {
  margin-top: 10rpx;
  display: block;
  align-content: center;
}

.top_image{
    display: table-column
}

.image{
    width: 88rpx; 
    height: 88rpx;
    border-radius: 44rpx; 
    background-color: #eeeeee;
}

.contact_item_mark {
  text-align: center;
}

.contact_item_text{
  color: #000000;
  font-family:"微软雅黑";
  font-weight:bold;
  line-height: 10rpx;
  font-size: 22rpx;
}

.sep_line {
  height: 36rpx;
  width: 100%;
  background-color: #F8F9FA;
}


.contact_table{
  display: flex;
  flex-direction: column;
  background-color: #FFFFFF;
}

.contact_table_cell {
  padding-left: 32rpx;
  margin-top: 7rpx;
  margin-bottom: 7px;
}

.contact_cell_image{
    width: 24rpx; 
    height: 24rpx;
}

.contact_cell_title{
  color: #000000;
  font-family:"微软雅黑";
  font-size:22rpx;
  padding-left: 30rpx;
}
.contact_cell_count{
    position: absolute;
    right: 15rpx;
    color:#808080;
    font-size: 18rpx;
    text-align: right center ;
}

.line-narrow{
    height: 1px;
    width:100%;
    background-color: #E0E1E2;
}